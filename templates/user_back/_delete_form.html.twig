<form method="post" action="{{ path('app_user_back_delete', {'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this User?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn btn-inverse-danger btn-fw">Delete</button>
</form>
